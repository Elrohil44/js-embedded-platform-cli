{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id", "_fd", "_processFile"] }]
  }
}